set(PACKAGE_VERSION3 "@LIBCHK_VERSION_MAJOR@.@LIBCHK_VERSION_MINOR@.@LIBCHK_VERSION_PATCH@")
set(PACKAGE_VERSION4 "${PACKAGE_VERSION3}.@LIBCHK_VERSION_TWEAK@")
set(PACKAGE_VERSION "${PACKAGE_VERSION4}")

if ("${PACKAGE_VERSION}" VERSION_LESS "${PACKAGE_FIND_VERSION}")
   set(PACKAGE_VERSION_COMPATIBLE FALSE) 
else()
  set(PACKAGE_VERSION_COMPATIBLE TRUE) # 1.x compatible
  if ("${PACKAGE_VERSION}" VERSION_EQUAL "${PACKAGE_FIND_VERSION}")
    set(PACKAGE_VERSION_EXACT TRUE) # 1.1.x exact match
  endif()
endif()

